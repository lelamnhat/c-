#include <bits/stdc++.h>
#include <string.h>
using namespace std;
long long a;
long long b;
void nhap(){
cout<<"nhap so a ";
cin>>a;
cout<<"nhap so b ";
cin>>b;
}
int check(int x, int y){
    if(x>y){
        return 1;
    }
    else{
        return 0;
    }
}
void ketqua(){
    if (check(a,b)==1){
        cout<<a<<">"<<b;
    }   
    else{
        cout<<a<<"<"<<b;
    }
}

int main(){
    nhap();
    ketqua();

   

    return 0;
}
