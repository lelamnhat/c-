#include <bits/stdc++.h>
#include <string.h>
using namespace std;

int n;

void nhap(){ 
    cout << "nhap so n ";
    cin >> n;
    while (n<5 || n>10){
        cout << "nhap lai so n ";
        cin >> n;
    }
}

void ketqua(int x){ 
    for(int i = 0; i <= x; i++){
        cout << i;
    }
}

int main(){
    nhap();
    ketqua(n); 
    return 0;
}
