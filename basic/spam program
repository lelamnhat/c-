#include <bits/stdc++.h>
#include <string.h>
using namespace std;

int n;

void nhap(){ 
    cout << "nhap so n ";
    cin >> n;
    while (n<0 || n>10000000000000){
        cout << "nhap lai so n ";
        cin >> n;
    }
}
void ketqua(int x){ 
    for(int i = 0; i <= x; i++){
        if (i % 2 == 0 & i % 3 == 0){
            cout <<"buff sao"<< i<<" " ;
        }
    }
    
}

int main(){
    nhap();
    ketqua(n);
    
    return 0;
}
